<!--template match = bibRefCitation--><xref rid="ref17428">Escolà 1971</xref>
, 
<!--template match = bibRefCitation--><xref rid="ref17452">1973</xref>
, 
<!--template match = bibRefCitation--><xref rid="ref17480">1980</xref>
, 
