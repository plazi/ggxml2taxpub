<quantity id="6F18EABAFFCBFFFA5A66FA01FE175619" box="[357,450,1528,1555]" metricMagnitude="-3" metricUnit="m" metricValue="2.5" pageId="8" pageNumber="42" unit="mm" value="2.5">2.5 mm</quantity>
