<paragraph id="8BDA36A9FFA9FFB3FF34FCCBFEBBFCAA" blockId="100.[189,1398,783,878]" pageId="100" pageNumber="101">
<taxonomicName id="4C654D2AFFA9FFB3FF34FCCBFE65FCED" authorityName="Korn &amp; Ghaderi" authorityYear="2016" box="[189,467,783,809]" class="Cephalopoda" family="Dzhulfitidae" genus="Stoyanowites" kingdom="Animalia" order="Ammonoidea" pageId="100" pageNumber="101" phylum="Mollusca" rank="species" species="aspinosus">
