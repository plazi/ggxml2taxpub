<typeStatus id="54EB88310367EF791CC27AB2FCE8689A" box="[758,809,1473,1499]" pageId="24" pageNumber="92">type</typeStatus>
species, 
<taxonomicName id="4C504D100367EF791DA77AB2FBB5689B" baseAuthorityName="Zhang, Song &amp; Zhu" baseAuthorityYear="2001" box="[915,1140,1472,1498]" class="Arachnida" family="Gnaphosidae" genus="Marjanus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Araneae" pageId="24" pageNumber="92" phylum="Arthropoda" rank="species" species="platnicki">
<emphasis id="B924EA810367EF791DA77AB2FBB5689B" box="[915,1140,1472,1498]" italics="true" pageId="24" pageNumber="92">Marjanus platnicki</emphasis>
</taxonomicName>
(Zhang, Song &amp; Zhu, 2001), distributed in 
<collectingCountry id="F34776030367EF791F837A94FDCB6B40" box="[439,522,1511,1537]" name="Greece" pageId="24" pageNumber="92">Greece</collectingCountry>
and 
<collectingCountry id="F34776030367EF791C727A94FD4D6B40" box="[582,652,1511,1537]" name="China" pageId="24" pageNumber="92">China</collectingCountry>
(
<bibRefCitation id="EFC14B620367EF791CA97A94FCE76B40" author="WSC" box="[669,806,1511,1537]" pageId="24" pageNumber="92" refId="ref18338" refString="WSC. 2021. World Spider Catalog. Version 21.5. Natural History Museum Bern. Available from http: // wsc. nmbe. ch [accessed 10 Jan. 2021]. https: // doi. org / 10.24436 / 2" type="url" year="2021">WSC 2021</bibRefCitation>
