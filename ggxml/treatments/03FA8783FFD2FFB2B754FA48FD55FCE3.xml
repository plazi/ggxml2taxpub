<specimenCode id="DBF59EEEFFD3FF8DB610FE99FB9AFE28" box="[788,1042,379,406]" collectionCode="NHMUK" country="United Kingdom" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34665" lsid="urn:lsid:biocol.org:col:34665" name="Natural History Museum, London" pageId="31" pageNumber="105" type="Museum">NHMUK 013380328</specimenCode>
