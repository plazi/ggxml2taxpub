<taxonomicName id="C0EAD9E0FFCBFF81A463F86FFBDDDB98" box="[883,1051,1919,1946]" class="Arachnida" family="Charinidae" genus="Charinus" kingdom="Animalia" order="Amblypygi" pageId="82" pageNumber="83" phylum="Arthropoda" rank="species" species="rocamadre">
