<taxonomicName id="E0FF4D0CB34EFFCF40ABFDC8FEA2FD9B" authorityName="Triapitsyn" authorityYear="2021" box="[189,357,583,610]" class="Insecta" family="Mymaridae" genus="Ganomymar" kingdom="Animalia" order="Hymenoptera" pageId="10" pageNumber="137" phylum="Arthropoda" rank="species" species="libertatium">
