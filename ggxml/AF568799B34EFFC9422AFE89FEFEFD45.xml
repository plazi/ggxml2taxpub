<taxonomicName id="E0FF4D0CB34EFFCF42CFFDA4FCF0FDC4" authorityName="De Santis" authorityYear="1972" box="[729,823,548,574]" class="Insecta" family="Mymaridae" genus="Ganomymar" kingdom="Animalia" order="Hymenoptera" pageId="10" pageNumber="137" phylum="Arthropoda" rank="species" species="dessarti">
