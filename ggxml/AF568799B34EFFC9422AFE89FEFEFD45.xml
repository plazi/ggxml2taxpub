<taxonomicName id="E0FF4D0CB34EFFCF40ABFDA4FE78FDC4" authorityName="De Santis" authorityYear="1972" box="[189,447,548,574]" class="Insecta" family="Mymaridae" genus="Ganomymar" kingdom="Animalia" order="Hymenoptera" pageId="10" pageNumber="137" phylum="Arthropoda" rank="species" species="dessarti">
