Gurney
</taxonomicName>
, 
<taxonomicName id="B2A1EBA8BF09FF845B5A3745FDB3A028" _evidence="catalogs" _step="genera" authority="Gunther" authorityName="Gunther" box="[264,576,564,590]" class="Insecta" family="Phasmatidae" genus="Thaumatobactron" kingdom="Animalia" order="Phasmida" pageId="13" pageNumber="44" phylum="Arthropoda" rank="genus">
