</taxonomicName>
and 
<taxonomicName id="B2A1EBA8BF09FF84582B3745FC7BA028" _evidence="catalogs" _step="genera" authority="Boisduval." authorityName="Boisduval." box="[633,904,564,590]" class="Insecta" family="Phasmatidae" genus="Eurycantha" kingdom="Animalia" order="Phasmida" pageId="13" pageNumber="44" phylum="Arthropoda" rank="genus">
