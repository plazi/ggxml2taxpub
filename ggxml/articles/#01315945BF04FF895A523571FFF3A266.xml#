<number id="C369820CBF0FFF8259AB37F5FBC4A0F8" box="[1017,1079,644,670]" pageId="11" pageNumber="42" value="1872.0">1872</number>
, pl. 
<number id="C369820CBF0FFF825E3E37F5FB7EA0F8" box="[1132,1165,644,670]" pageId="11" pageNumber="42" value="10.0">10</number>
