(Type-species: 
<taxonomicName id="B2A1EBA8BF09FF845E5234E5FDA1A3B0" _evidence="catalogs" _step="species1" authority="Gunther, 1929: 663" authorityName="Gunther" authorityPageNumber="663" authorityYear="1929" class="Insecta" family="Phasmatidae" genus="Thaumatobactron" kingdom="Animalia" order="Phasmida" pageId="13" pageNumber="44" phylum="Arthropoda" rank="species" species="poecilosoma">
