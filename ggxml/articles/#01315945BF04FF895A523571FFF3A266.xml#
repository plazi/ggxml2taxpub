; 
<taxonomicName id="B2A1EBA8BF04FF8958813358FC93A426" _evidence="catalogs" _step="nonPrimaryHigher" box="[723,864,1577,1600]" class="Insecta" family="Phasmatidae" kingdom="Animalia" order="Phasmida" pageId="0" pageNumber="31" phylum="Arthropoda" rank="tribe" tribe="Eurycanthini">Eurycanthini</taxonomicName>
