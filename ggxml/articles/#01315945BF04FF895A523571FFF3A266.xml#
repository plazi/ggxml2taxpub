<taxonomicName id="B2A1EBA8BF09FF845B9734E5FCB4A3C8" _evidence="catalogs" _step="genera" authority="Gunther, 1929" authorityName="Gunther" authorityYear="1929" box="[453,839,404,430]" class="Insecta" family="Phasmatidae" genus="Thaumatobactron" kingdom="Animalia" order="Phasmida" pageId="13" pageNumber="44" phylum="Arthropoda" rank="genus">
