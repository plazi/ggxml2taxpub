described 
<taxonomicName id="B2A1EBA8BF0FFF8258E2378DFC8DA173" _evidence="catalogs" _step="species1" box="[688,894,764,789]" class="Insecta" family="Heteropterygidae" genus="Eubulides" kingdom="Animalia" order="Phasmida" pageId="11" pageNumber="42" phylum="Arthropoda" rank="species" species="spuria">
