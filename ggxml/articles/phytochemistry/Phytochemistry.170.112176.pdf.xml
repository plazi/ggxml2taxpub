<location box="[818,1116,1825,1844]" country="Macao" county="The" municipality="Phytochemicals in Medicine" name="Association of Dietetic Nutrition" pageId="0" pageNumber="1" stateProvince="Shaanxi">Association of Dietetic Nutrition</location>
and 
<location box="[1163,1220,1825,1844]" country="Macao" county="The" municipality="Phytochemicals in Medicine" name="Safety" pageId="0" pageNumber="1" stateProvince="Shaanxi">Safety</location>
