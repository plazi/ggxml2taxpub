<taxonomicName id="A4C698C4FFBBFF92ED6EF86037E1F866" authority="Ramil &amp; Vervoort, 1992" authorityName="Ramil &amp; Vervoort" authorityYear="1992" box="[586,1165,1925,1952]" class="Hydrozoa" family="Tiarannidae" genus="Stegopoma" kingdom="Animalia" order="Leptothecata" pageId="9" pageNumber="58" phylum="Cnidaria" rank="species" species="giganteum">
