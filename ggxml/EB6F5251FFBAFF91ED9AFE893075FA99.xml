<taxonomicName id="A4C698C4FFB8FF91EF99FAA3322AFAA6" authorityName="Quelch" authorityYear="1885" box="[189,326,1349,1375]" class="Hydrozoa" family="Lafoeidae" genus="Zygophylax" kingdom="Animalia" order="Leptothecata" pageId="10" pageNumber="59" phylum="Cnidaria" rank="genus">
