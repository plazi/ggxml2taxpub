? 
<taxonomicName id="A4C698C4FFB8FF91EE46FAA332A2FAA6" authorityName="Billard" authorityYear="1905" box="[354,462,1349,1375]" class="Hydrozoa" family="Lafoeidae" genus="Zygophylax" kingdom="Animalia" order="Leptothecata" pageId="10" pageNumber="59" phylum="Cnidaria" rank="species" species="biarmata">
