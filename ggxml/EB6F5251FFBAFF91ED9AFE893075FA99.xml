<quantity id="A43E4EA2FFB8FF91ED52FAA33079FA99" box="[630,789,1349,1376]" metricMagnitude="2" metricUnit="m" metricValue="2.485" metricValueMax="3.3299999999999996" metricValueMin="1.64" pageId="10" pageNumber="59" unit="m" value="248.5" valueMax="333.0" valueMin="164.0">
164 to 
<specimenCount id="75C028CEFFB8FF91EDE3FAA33079FA99" box="[711,789,1349,1376]" pageId="10" pageNumber="59" type="generic">333 m</specimenCount>
