<quantity id="9E5451D7B164FFFEFEC76C6CFE9FFCAC" box="[288,367,846,867]" metricMagnitude="3" metricUnit="m" metricValue="1.7" pageId="1" pageNumber="352" unit="m" value="1700.0">1700 m</quantity>
