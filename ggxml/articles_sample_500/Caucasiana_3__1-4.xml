<taxonomicName id="62AFECA65EAC2981B9234CFE6676B6ED" class="Insecta" genus="H." higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="H. wittmeri" order="Psocoptera" pageId="0" pageNumber="1" phylum="Arthropoda" rank="species" species="wittmeri">
